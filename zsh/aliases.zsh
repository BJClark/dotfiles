alias reload!='. ~/.zshrc'

alias cls='clear' # Good 'ol Clear Screen command

alias ch_list='chamber list $(cat /var/tmp/interim.name)'
alias ch_read='chamber read $(cat /var/tmp/interim.name)'
alias ch_write='chamber write $(cat /var/tmp/interim.name)'
alias ch_delete='chamber delete $(cat /var/tmp/interim.name)'
